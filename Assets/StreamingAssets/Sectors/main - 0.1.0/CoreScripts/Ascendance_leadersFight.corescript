        ConditionBlock(
            Comparison(
                val1=$$Ascendance_flagCnt,
                val2=17,
                comp=Gt,
                sequence=(
                    SetVariable(name=$$Ascendance_flagCnt, value=1)
                )
            ),
            Comparison(
                val1=$$Ascendance_flagCnt,
                val2=18,
                comp=Lt,
                sequence=(

                )
            ),
        ),


        ConcatenateValues(name=$$Ascendance_flag, val1=Ascendance-final-flag, val2=$$Ascendance_flagCnt),

        SpawnEntity(
            entityID=$$Ascendance_ID,
